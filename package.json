{
  "name": "financial-flow-tracker-frontend",
  "private": true,
  "version": "0.1.0",
  "description": "Front-end for the FinancialFlowTracker application",
  "author": "Oleh Yaremyn <oleh_yaremyn@ukr.net>",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "npm": "9.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn typecheck && vite build",
    "optimize": "vite optimize",
    "preview": "vite preview --debug",
    "preview:test": "start-server-and-test preview http-get://localhost:3000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:hook": "yarn test --coverage=false",
    "e2e:run": "yarn preview:test 'cypress run --e2e'",
    "e2e:open": "yarn preview:test 'cypress open --e2e --browser chrome'",
    "e2e:hook": "yarn e2e:run",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --cache --fix",
    "lint:hook": "yarn lint:fix && yarn lint --quiet",
    "format": "prettier \"./**/*.{ts,tsx,scss,md,json}\"",
    "format:check": "yarn format --ignore-unknown --check",
    "format:fix": "yarn format --write --ignore-unknown --cache",
    "format:hook": "yarn format:fix && yarn format --ignore-unknown --list-different",
    "styles": "stylelint \"**/*.scss\"",
    "styles:fix": "yarn styles --fix --cache",
    "styles:hook": "yarn styles:fix && yarn styles --quiet",
    "lint:format:styles:check": "yarn lint && yarn styles && yarn format",
    "lint:format:styles:fix": "yarn lint:fix && yarn styles:fix && yarn format:fix",
    "lint:format:styles:hook": "yarn format:hook && yarn styles:hook && yarn lint:hook",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.11",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "i18next": "^22.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "cypress": "^12.12.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.23",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.6.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olehyaremyn7/financial-flow-tracker-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/olehyaremyn7/financial-flow-tracker-frontend.git/issues"
  },
  "homepage": "https://github.com/olehyaremyn7/financial-flow-tracker-frontend.git/#readme",
  "keywords": [
    "ReactJS",
    "React Hooks",
    "TypeScript",
    "Redux Toolkit",
    "React Query",
    "React Router",
    "React-Redux",
    "axios",
    "react-i18next",
    "i18next",
    "tailwindcss",
    "postcss",
    "sass",
    "Vite",
    "Vitest",
    "Cypress",
    "React Testing Library",
    "eslint",
    "prettier",
    "stylelint",
    "axe-core",
    "husky",
    "yarn"
  ]
}
