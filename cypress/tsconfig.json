{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "allowJs": false,
    "sourceMap": false,
    "composite": true,
    "types": ["cypress", "node"]
  },
  "include": ["**/*.ts", ".eslintrc.json", "../cypress.config.ts"]
}
